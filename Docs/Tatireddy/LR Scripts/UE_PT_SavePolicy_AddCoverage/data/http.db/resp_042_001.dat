function ValidateClaimant(button,claimStatus)
{
     if (button.name == 'save')
     {
        if(claimStatus == 'TRIAGE'){
              $('span[name="claimGroupradio"]').removeClass('required');
              $('span[name="claimGroupradio"]').addClass('notRequired');
        }
        else {
              $('span[name="claimGroupradio"]').removeClass('notRequired');
              $('span[name="claimGroupradio"]').addClass('required');
        }
     }
     else {
              $('span[name="claimGroupradio"]').removeClass('notRequired');
              $('span[name="claimGroupradio"]').addClass('required');
     }

     var clmid = '70433';
	if (claimStatus == 'Closed' || claimStatus == 'CCH' || claimStatus == 'CWP')
	{
		var errormess = '';

		if (($('input[name=LOB_IDList]').val().indexOf('ELL') &gt; -1) || ($('input[name=LOB_IDList]').val().indexOf('ML') &gt; -1)){
			if ($('#wrongfullAct1').val() == 'WTBD'){
					errormess += 'Wrongful Act can not be TBD to close. ';
			}
		}
		if ($('input[name=LOB_IDList]').val().indexOf('GL') &gt; -1){
			if ($('#lossLocation1').val() == 'LTBD'){
					errormess += 'Loss Location can not be TBD to close. ';
			}
			if ($('#additionalFactor1').val() == 'ATBD'){
					errormess += 'Additional Factors can not be TBD to close. ';
			}
			if ($('#cDEG_InjTypeCode1').val() == 'ITBD'){
					errormess += 'Injury Driving the Claim can not be TBD to close. ';
			}
		}
		if (($('input[name=LOB_IDList]').val().indexOf('ELL') &gt; -1) || ($('input[name=LOB_IDList]').val().indexOf('GL') &gt; -1) || ($('input[name=LOB_IDList]').val().indexOf('ML') &gt; -1)){
			if ($('#firstCause').val() == 'CTBD'){
					errormess += 'Cause of Loss can not be TBD to close. ';
			}
		}

		Communication.CustomRequest('claims_modify2.max?AJAX_ACTION=checkCLrole&amp;CLMID=' + clmid, function (AjaxResp)
		{
			if (AjaxResp &gt; 0){
			  errormess += 'Claimant Role for claimant is required and can not be TBD from Claimant page. ';
			}

			if (errormess != ''){
				jAlert(errormess, "Invalid Entry");
				return;
			}

			if ( ($('input[type=radio][name=reportedToReinsurer][value=1]').is(':checked')) &amp;&amp; ($.inArray(claimStatus, ['Closed','CCH','CWP']) == -1)
				&amp;&amp; ((claimStatus != 'TRIAGE') || ($(button).attr('name') == 'triage')  ) )
			{
				var adjuster = $('input[name=cCD_ClaimAdjuster]:checked').val();
				Communication.CustomRequest('claims_modify2.max?AJAX_ACTION=ReportReinsur&amp;TCLMID=' + clmid + '&amp;adjuster=' + adjuster + '');
			}

			Communication.SerialRequest($("#rightColumn"), false, button);
		});
	}
	else
	{
		if ( ($('input[type=radio][name=reportedToReinsurer][value=1]').is(':checked')) &amp;&amp; ((claimStatus != 'TRIAGE') || ($(button).attr('name') == 'triage')  ) )
		{
			var adjuster = $('input[name=cCD_ClaimAdjuster]:checked').val();
			Communication.CustomRequest('claims_modify2.max?AJAX_ACTION=ReportReinsur&amp;TCLMID=' + clmid + '&amp;adjuster=' + adjuster + '');
		}

