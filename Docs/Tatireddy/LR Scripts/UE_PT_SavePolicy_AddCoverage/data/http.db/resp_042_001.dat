function ContainsAll(subsetitem, setitem){
  for(var i = 0 , len = subsetitem.length; i &lt; len; i++){
     if($.inArray(subsetitem[i], setitem) == -1) return false;
  }
  return true;
}

function StyleControl(lControl, control, sType){
   switch(sType) {
      case 'required':
          lControl.removeClass('notRequired');
          lControl.addClass('required');
          control.removeAttr('disabled');
          control.removeClass('notRequired');
          break;
      case 'disabled':
          lControl.removeClass('notRequired');
          lControl.removeClass('required');
          lControl.removeClass('Default');
          lControl.removeClass('DefaultBold');
          control.removeClass('required');
          control.removeClass('notRequired');
          control.attr('disabled', 'disabled');
          break;

      case 'optional':
          lControl.removeClass('required');
          lControl.addClass('notRequired');
          control.removeAttr('disabled');
          control.removeClass('required');
          break;
   }
}

//This defaults the value of report to reinsurer button to yes if the product is GLX and the user selection is in the reinsurerArray
function repToReinsurerSetter()
{
       var dateOfLoss = new Date('04/04/2016');
       var markDate = new Date('1/1/2015');
       var productValues = '';
       var minorCount =  '0';

       //if($("#PolList").dataTable().fnSettings().aoData.length != 0) {
          $("input:checked", $("#PolList").dataTable().fnGetNodes()).each(function () {
                productValues += $(this).attr('productv') + ',';
          });

          productValues = productValues.slice(0,-1);
          var injuryArrayPrior = ['FATAL', 'AMP', 'PARA'];
          var injuryArrayAfter = ['AMP', 'PARA'];
          var COLArray = ['ASSAUL2','ASSAUL1'];

          var injurySelection = $('#cDEG_InjTypeCode1').val();
          var COLSelection = $('#firstCause').val();

          //if reinsurance is already yes then exit
          if($('#hSelectedReinsuer').val() == '1' )
              return;

          //When the Loss Date is prior to 1/1/2015 AND the Injury Driving the Claim is is selected as either; fatality, Amputation, Paralysis.
          //OR the Loss Date is prior to 1/1/2015 AND the Cause of Loss is selected as either Sexual Assault- Perp over 18 or Sexual Assault- Per &lt;18.
          //OR the Loss Date is after to 1/1/2015 AND Injury Driving the Claim is selected as either Paralysis, Amputation.
          //OR the Loss Date is after to 1/1/2015 AND the Cause of Loss is selected as either Sexual Assault- Perp over 18 or Sexual Assault- Per &lt;18
          //AND the Claimant Role is selected as either Minor or Student &lt;18.
          if(((productValues.search('GLX') != '-1') || (productValues.search('GLU') != '-1'))  &amp;&amp;
            ((dateOfLoss &lt; markDate &amp;&amp; jQuery.inArray(injurySelection, injuryArrayPrior) != '-1') ||
             (dateOfLoss &lt; markDate &amp;&amp; jQuery.inArray(COLSelection, COLArray)!= '-1') ||
             (dateOfLoss &gt; markDate &amp;&amp; jQuery.inArray(injurySelection, injuryArrayAfter) != '-1') ||
             (dateOfLoss &gt; markDate &amp;&amp; jQuery.inArray(COLSelection, COLArray)!= '-1' &amp;&amp; minorCount &gt; 0)))
          {
             //productValues.search("General Liability Excess");
             $('[name=reportedToReinsurer][value=1]').attr('checked','checked');
          }
          else {
             $('[name=reportedToReinsurer][value=0]').attr('checked','checked');
          }
       //}
}

