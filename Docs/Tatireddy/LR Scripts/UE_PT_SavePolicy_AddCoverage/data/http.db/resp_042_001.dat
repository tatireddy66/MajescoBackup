function checkIfClosed()
{
    var isBilling = '1';
    if($('#claimStatus').val() == 'Open') {
         $('#closeReason').parent().hide();
    }
    else if ( $('#claimStatus').val() == 'Closed') {
        var userID = '16769';      
            $('#spcloseReason').removeClass('notRequired');
            $('#spcloseReason').addClass('required');
            $('#closeReason').parent().show();        
        Communication.CustomRequest('claims_modify2.max?AJAX_ACTION=CheckBillGroup&amp;USERID=' + userID, function(AjaxResp){
             var result = JSON.parse(AjaxResp);
             if(result.IsBill != '1') {
                 jAlert('Only user from billing group can select &lt;strong&gt;' + $('#claimStatus :selected').text() + '&lt;/strong&gt;!');
                 $('#claimStatus').val($('#hStatusID').val());
                 CustomScript.ShowHIdeStatus();
             }
        });
    }
    else if ( $('#claimStatus').val() == 'CWP') {
        var clmid = '70433';
            $('#spcloseReason').removeClass('notRequired');
            $('#spcloseReason').addClass('required');
            $('#closeReason').parent().show();

        Communication.CustomRequest('claims_modify2.max?AJAX_ACTION=PaymentExist&amp;CLMID=' + clmid, function(AjaxResp){
             var result = JSON.parse(AjaxResp);
             if(result.IsFinancialCheck != '1' &amp;&amp; isBilling != '1') {
                 jAlert('This claim has payment, cannot be closed as &lt;strong&gt;' + $('#claimStatus :selected').text() + '&lt;/strong&gt; status!');
                 $('#claimStatus').val($('#hStatusID').val());
                 CustomScript.ShowHIdeStatus();
             }
        });

    }
    else if ($('#claimStatus').val() == 'CCH') {
        $('#spcloseReason').removeClass('notRequired');
        $('#spcloseReason').addClass('required');
        $('#closeReason').parent().show();

        Communication.CustomRequest('claims_modify2.max?AJAX_ACTION=statusisCCH', function(AjaxResp){
              var message = '';
              var result = JSON.parse(AjaxResp);

              //IsELLFlag
              if(result.IsELLFlag == '1') {
                  message += ' \u2022 The Claim contains ELL. Please complete Cause of Loss, Claimant Role and Wrongful Act.\n'
              }

              //IsGLFlag
              if(result.IsGLFlag == '1') {
                  message += ' \u2022 The Claim contains GL. Please complete Cause of Loss, Claimant Role, Loss Location, Additional Factor and Injury Code.\n'
              }

              //IsReinsurerFlag
              if(result.IsReinsurerFlag == '1') {
                  message += ' \u2022 Please make sure reinsurance reporting form is completed and sent to Guy Carpenter.\n'
              }

              if(message != '')
                 jAlert(message);
        });
    } else
    {
        $('#closeReason').parent().hide();
    }
}
