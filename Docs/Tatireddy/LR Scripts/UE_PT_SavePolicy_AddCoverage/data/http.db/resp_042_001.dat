    {
        $('#dialog #AttachEmailAddress').val($.trim($('#dialog #AttachEmailAddress').val() + ' ' + emailadd + ';'));
        $('#dialog #Email').val('');
    }
}

function removeEmail()
{
    var Original_Email_List = $('#dialog #AttachEmailAddress').val();

    if (Original_Email_List.match(/\S+/g).length == 1)
    {
        var Email_To_Remove = Original_Email_List.match(/[^ ]*$/);
    } else
    {
        var Email_To_Remove = ' ' + Original_Email_List.match(/[^ ]*$/);
    }

    var Email_Update = Original_Email_List.replace(Email_To_Remove, '');

    $('#dialog #AttachEmailAddress').val($.trim(Email_Update));

}

