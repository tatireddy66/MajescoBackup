function addEmail()
{
    var emailadd = $('#dialog #Email').val().match(/([a-zA-Z0-9._-]+@[a-zA-Z0-9._-]+\.[a-zA-Z0-9._-]+)/gi);

    if (emailadd == null)
    {
        alert('Please use a valid email address.');
