//this enforces the uniqueness of the cause of loss drop down lists
function enforceCOLUnique()
{
    var COL1 = $('#firstCause').val();
    var COL2 = $('select[name=secondCause]').val();
    var COL3 = $('#thirdCause').val();
    $('#firstCause option').removeAttr('disabled');
    $('select[name=secondCause] option').removeAttr('disabled');
    $('#thirdCause option').removeAttr('disabled');
    if (COL1 != '-1')
    {
        $('select[name=secondCause] option[value=' + COL1 + ']').attr('disabled', 'disabled');
        $('#thirdCause option[value=' + COL1 + ']').attr('disabled', 'disabled');
    }
    if (COL2 != '-1')
    {
        $('#firstCause option[value=' + COL2 + ']').attr('disabled', 'disabled');
        $('#thirdCause option[value=' + COL2 + ']').attr('disabled', 'disabled');
    }
    if (COL3 != '-1')
    {
        $('select[name=secondCause] option[value=' + COL3 + ']').attr('disabled', 'disabled');
        $('#firstCause option[value=' + COL3 + ']').attr('disabled', 'disabled');
    }
}

