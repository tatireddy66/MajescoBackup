function unique(list) {
    var result = [];
    $.each(list, function(i, e) {
        if ($.inArray(e, result) == -1) result.push(e);
    });

    return result;
}

//Update selected policy information
function UpdateSelectedPolicyInfomation(covtrigger, pdid, pdcid, productValue, endoValue, sChecked){
    
    var lobValues = '';
    var endorsementsValues = '';
    var productValues = '';
    var pdids = '';
    var pdcids = '';

    var lobValue = $(sChecked).val();

    if ($(sChecked).is(':checked')) {
        //Adding selected policy to the list
        pdids = $('input[name=PolicyIDList]').val();
        //&amp;&amp; pdids.split(",").indexOf(pdid) ==-1 remove because unable to save more than one product that belongs to the same policy (PD_ID)
        if(pdids!=undefined)
        {
            pdids = pdids ? (pdids + ',' + pdid) : pdid;
        }

        $('input[name=PolicyIDList]').val(pdids);
        pdids = pdids.split(',');

        covtriggers = $('input[name=CovTriggerList]').val();
        //&amp;&amp; covtriggers.split(",").indexOf(covtrigger) ==-1
        if(covtriggers!=undefined )
        {
           covtriggers = covtriggers ? (covtriggers + ',' + covtrigger) : covtrigger;
        }
        $('input[name=CovTriggerList]').val(covtriggers);
        covtriggers = covtriggers.split(',');

        pdcids = $('input[name=PDC_IDList]').val();
        //&amp;&amp; pdcids.split(",").indexOf(pdcid) ==-1
        if(pdcids!=undefined)
        {
           pdcids = pdcids ? (pdcids + ',' + pdcid) : pdcid;
        }
        $('input[name=PDC_IDList]').val(pdcids);
        pdcids = pdcids.split(',');

        lobValues = $('input[name=LOB_IDList]').val();
        //&amp;&amp; lobValues.split(",").indexOf(lobValue) ==-1
        if(lobValues!=undefined )
        {
           lobValues = lobValues ? (lobValues + ',' + lobValue) : lobValue;
        }
        $('input[name=LOB_IDList]').val(lobValues);
        lobValues = lobValues.split(',');

        productValues = $('input[name=ProductList]').val();
        //&amp;&amp; productValues.split(",").indexOf(productValue) ==-1
        if(productValues!=undefined)
        {
          productValues = productValues ? (productValues + ',' + productValue) : productValue;
        }
        $('input[name=ProductList]').val(productValues);
        productValues = productValues.split(',');

        endorsementsValues = $('input[name=ProductList]').val();
        //&amp;&amp; endorsementsValues.split(",").indexOf(endoValue) ==-1
        if(endorsementsValues!=undefined)
        {
           endorsementsValues = endorsementsValues ? (endorsementsValues + ',' + endoValue) : endoValue;
        }
        $('input[name=EndorsementsList]').val(endorsementsValues);
        endorsementsValues = endorsementsValues.split(',');
    }
    else {
        //Removing selected policy from the list
        if ($('input[name=PolicyIDList]').val().length !== 0){
           pdids = $('input[name=PolicyIDList]').val().split(',');

           var index = pdids.indexOf(pdid);
           if (index !== -1) {
               pdids.splice(index, 1);
               $('input[name=PolicyIDList]').val(pdids.join(','));
           }
        }

         if ($('input[name=CovTriggerList]').val().length !== 0){
           covtriggers = $('input[name=CovTriggerList]').val().split(',');

           var index = covtriggers.indexOf(covtrigger);
           if (index !== -1) {
               covtriggers.splice(index, 1);
               $('input[name=CovTriggerList]').val(covtriggers.join(','));
           }
        }

        if($('input[name=PDC_IDList]').val().length !==0){
            pdcids = $('input[name=PDC_IDList]').val().split(',');

            var index = pdcids.indexOf(pdcid);
            if (index !== -1) {
                pdcids.splice(index, 1);
                $('input[name=PDC_IDList]').val(pdcids.join(','));
            }
        }

        if($('input[name=LOB_IDList]').val().length !==0){
            lobValues = $('input[name=LOB_IDList]').val().split(',');

            var index = lobValues.indexOf(lobValue);
            if (index !== -1) {
                lobValues.splice(index, 1);
                $('input[name=LOB_IDList]').val(lobValues.join(','));
            }
        }

        if($('input[name=ProductList]').val().length !==0){
            productValues = $('input[name=ProductList]').val().split(',');

            var index = productValues.indexOf(productValue);
            if (index !== -1) {
                productValues.splice(index, 1);
                $('input[name=ProductList]').val(productValues.join(','));
            }
        }

        if($('input[name=EndorsementsList]').val().length !== 0){
            endorsementsValues = $('input[name=EndorsementsList]').val().split(',');

            var index = endorsementsValues.indexOf(endoValue);
            if (index !== -1) {
                endorsementsValues.splice(index, 1);
                $('input[name=EndorsementsList]').val(endorsementsValues.join(','));
            }
        }
    }

    //ZE-- Hack for RPS until this gets refactored. This is an urgent production issue that needs to be fixed ASAP.
    if ( lobValues.includes("RPS") )
    {
       lobValues.length = 0;  //This works better than lobValues = [] in case there is any references to the orginal array in memory.

       if ( productValues.includes("SBC") || productValues.includes("SBO") || productValues.includes("SCF") || productValues.includes("SOF") )  //business rule treating SBC as the ML line
            lobValues.push("ML");

       if ( productValues.includes("SAL") || productValues.includes("SGL") || productValues.includes("SAF") )   //business rule treating SAL and SGL as the GL line
            lobValues.push("GL");
    }

    //This is for LOB RPH
    if ( lobValues.includes("RPH") )
    {
       lobValues.length = 0;

       //Business rule for HCL is the same as ELL
       if ( productValues.includes("HCM") )
            lobValues.push("ML");

       //Business rule for HOC is the same as GL
       if ( productValues.includes("HOC") )
            lobValues.push("GL");
    }

    return {
        lobValues: lobValues,
        endorsementsValues: endorsementsValues,
        productValues: productValues,
        pdids: pdids,
        pdcids: pdcids
    };
}

