function ToggleSection(el) {
    $(el).children('.stat').toggle();
    $(el).parent().children('div').toggle();
    $('#AGY_LIT_ENTRY').toggle();
}

function getObjects(obj, key, val) {
    var objects = [];
    for (var i in obj) {
        if (!obj.hasOwnProperty(i)) continue;
        if (typeof obj[i] == 'object') {
            objects = objects.concat(CustomScript.getObjects(obj[i], key, val));
        } else
        //if key matches and value matches or if key matches and value is not passed
        //(eliminating the case where key matches but passed value does not)
        if (i == key &amp;&amp; obj[i] == val || i == key &amp;&amp; val == '') { //
            objects.push(obj);
        } else if (obj[i] == val &amp;&amp; key == ''){
            //only add if the object is not already in the array
            if (objects.lastIndexOf(obj) == -1){
                objects.push(obj);
            }
        }
    }
    return objects;
}

function MarkAsSelected(control, Cov_Trigger, PolicyID, egPDC_ID, Product, Endorsements) {
  try {
          var policyId=$(control).attr('policyv');

          Global.ShowProgress();
          if(policyId!='' &amp;&amp; control.checked==false)
          {
               var SyncRespComp = $.ajax({
                  type: "GET",
                  url: 'claims_modify2.max?AJAX_ACTION=UncheckPolicy&amp;CCD_ID=70433&amp;PD_ID='+ policyId + '&amp;t=' + Date.now(),
                  async: false
                }).responseText;

                //Parse the response into XML
                if (window.DOMParser)
                {
                   parser=new DOMParser();
                   xmlDocComp=parser.parseFromString(SyncRespComp, "text/xml");
                }
                else
                {
                   // Internet Explorer
                   xmlDocComp=new ActiveXObject("Microsoft.XMLDOM");
                   xmlDocComp.async=false;
                   xmlDocComp.loadXML(SyncRespComp);
                }

                //Get JSON from the response
                var respComp = JSON.parse(xmlDocComp.getElementsByTagName("html")[0].childNodes[0].nodeValue);

                if(respComp.CoverageCount &gt; 0)
                {
                    jAlert('This claim is associated with a coverage already. Please de-select coverage(s) before making changes to policy.');
                    $(control).attr('checked',true);
                }
          }

          $(this).is(':checked') ? $(control).parent().parent().find('td.PolInclude').text('Selected') : $(control).parent().parent().find('td.PolInclude').text('');
          CustomScript.pollistclick(Cov_Trigger, PolicyID, egPDC_ID, Product, Endorsements, control);
          Global.HideProgress();
     }
  catch(err)
  {
     Global.HideProgress();
  }
}


